{
  "$schema": "https://raw.githubusercontent.com/sumneko/vscode-lua/master/setting/schema.json",
  "runtime.version": "LuaJIT",
  "workspace": {
    "library": [
      "$VIMRUNTIME",
      "$LLS_Addons/luvit"
    ],
    "checkThirdParty": "Disable"
  },
  "diagnostics.disable": [
    "undefined-global"
  ],
  "format.enable": false
}
